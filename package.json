{
  "name": "trading-agent-system",
  "version": "1.0.0",
  "main": "src/server.ts",
  "scripts": {
    "server:build": "tsc",
    "server:build:watch": "tsc --watch",
    "server:dev": "tsx src/server.ts",
    "brain:start": "tsx startBrain.ts",
    "brain:paper": "ENABLE_AUTO_TRADING=false tsx startBrain.ts",
    "pipeline:scheduler": "tsx src/pipeline/scheduler.ts",
    "pipeline:test": "tsx src/pipeline/test_pipeline.ts",
    "agents:start": "./scripts/start-agents.sh",
    "agents:stop": "./scripts/stop-agents.sh",
    "agents:health": "./scripts/health-check.sh",
    "start:dev": "tsx src/start-agents.ts",
    "server:inspect": "DANGEROUSLY_OMIT_AUTH=true npx @modelcontextprotocol/inspector npm run server:dev"
  },
  "keywords": [
    "trading-bot",
    "ai-agents",
    "microservices"
  ],
  "author": "",
  "license": "ISC",
  "description": "A microservices-based AI trading agent system.",
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.17.5",
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.0.3",
    "concurrently": "^8.2.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.22",
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.13.0",
    "@supabase/supabase-js": "^2.86.2",
    "ai": "^5.0.107",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.2.1",
    "ioredis": "^5.8.2",
    "openai": "^6.10.0",
    "zod": "^3.25.67"
  }
}
